{
  "redirects": [
    {
      "source": "http://omegoo.chat/(.*)",
  "destination": "https://www.omegoo.chat/$1",
      "permanent": true
    },
    {
      "source": "http://www.omegoo.chat/(.*)",
  "destination": "https://www.omegoo.chat/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
